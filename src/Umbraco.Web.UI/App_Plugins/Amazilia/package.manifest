        "~/App_Plugins/Amazilia/Backoffice/amazilia.controller.js",
        "~/App_Plugins/Amazilia/Backoffice/amazilia.directives.js",
        "~/App_Plugins/Amazilia/Backoffice/amazilia.services.js",
